"collectCoverageFrom": [
  "src/**/*.{js,jsx,ts,tsx}",
  "!<rootDir>/node_modules/",
  "!<rootDir>/path/to/dir/"
],
"coverageThreshold": {
  "global": {
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "statements": 100
  }
},
"collectCoverage": true,
"coverageReporters": ["json", "html"],
"snapshotSerializers": ["my-serializer-module"]