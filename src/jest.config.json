"collectCoverageFrom": [
  "src/**/*.{js,jsx,ts,tsx}",
  "!<rootDir>/node_modules/",
  "!<rootDir>/path/to/dir/"
],
"coverageThreshold": {
  "global": {
    "branches": 110,
    "functions": 90,
    "lines": 90,
    "statements": 90
  }
},
"collectCoverage": true,
"coverageReporters": ["json", "html"],
"snapshotSerializers": ["my-serializer-module"]